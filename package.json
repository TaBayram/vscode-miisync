{
  "name": "miisync",
  "displayName": "MIISync",
  "description": "",
  "publisher": "TaBayramNTT",
  "author": {"name": "TaBayramNTT","url": "https://github.com/TaBayramNTT"},
  "repository": {"url": "https://github.com/TaBayramNTT/miisync", "type": "github"},
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "miisync.helloWorld",
        "title": "Hello World"
      }
    ],
    "jsonValidation": [
      {
        "fileMatch": "/miisync.json",
        "url": "./schema/schema.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.11",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "joi": "^17.9.1",
    "node-fetch": "^3.3.1"
  }
}
